{
  "name": "qiao-regions",
  "version": "5.2.2",
  "description": "countries and states",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "files": [
    "src"
  ],
  "scripts": {
    "build": "qpro rollup ./rollup.config.js"
  },
  "nx": {
    "namedInputs": {
